             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DrumSharp"
             mc:Ignorable="d" 
             d:DesignHeight="350" d:DesignWidth="525">
    <Grid>
        <Button x:Name="change_keybinds" Content="Change KeyBinds" Margin="200,90,200.143,0" VerticalAlignment="Top" Click="change_keybinds_Click"/>
        <Button x:Name="back" Content="Back" Margin="200,228,200.143,0" VerticalAlignment="Top" Click="back_Click"/>
    </Grid>
</UserControl>
